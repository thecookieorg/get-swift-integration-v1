<p id="notice"><%= notice %></p>

<h1>Listing Swift Api Keys</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @swift_api_keys.each do |swift_api_key| %>
      <tr>
        <td><%= swift_api_key.key %></td>
        <td><%= link_to 'Show', swift_api_key %></td>
        <td><%= link_to 'Edit', edit_swift_api_key_path(swift_api_key) %></td>
        <td><%= link_to 'Destroy', swift_api_key, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Swift api key', new_swift_api_key_path %>
